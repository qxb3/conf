      children: [
        /* @__PURE__ */ jsx(
          BatteryIcon,
          {
            className: charging ? "icon charging" : "icon discharging",
            charging
          }
        ),
        /* @__PURE__ */ jsx(
          "label",
          {
            className: "percentage",
            label: bind(battery2, "percentage").as(
              (percentage) => `${(percentage * 100).toFixed(0)}%`
            ),
            xalign: 0
          }
        )
      ]
    }
  );
}
